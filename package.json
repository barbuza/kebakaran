  },
  "devDependencies": {
    "asap": "^2.0.3",
    "babel-cli": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.4.3",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "faucet": "0.0.1",
